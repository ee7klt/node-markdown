<% include partials/header %>
  <a class="button" href="/blogs/new">NEW</a>
  <% blogs.forEach((x) => { %>
    <div class="container">
      <span><%= x.created.toDateString() %></span>
      <div class="postTitle">
        <h1><a href="/blogs/<%= x._id%>"><%= x.title %></a></h1>
      </div>
      <div class="modify">
        <form class='edit' style="display: inline;" action="/blogs/<%= x.id %>/edit" method="GET">
      <input type="submit" value="Edit">
      </form>
        <form class='delete' style="display: inline;" action="/blogs/<%= x.id %>?_method=DELETE" method="POST">
      <input type="submit" value="Delete">
      </form>
      </div>



      <div class="postBody">
        <%- x.body %>
      </div>

      <hr >
    </div>
    <% }) %>

  </body>
